{
    "name": "jumptwentyfour/laravel-coding-standards",
    "description": "Coding standards for our PHP applications.",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/JumpTwentyFour/php-coding-standards"
        }
    ],
    "authors": [
        {
            "name": "Alex Williams",
            "email": "alex@jump24.co.uk",
            "homepage": "https://jump24.co.uk",
            "role": "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "JumpTwentyFour\\LaravelCodingStandards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "JumpTwentyFour\\LaravelCodingStandards\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "nunomaduro/larastan": "^0.6.11|^0.7",
        "nikic/php-parser": "^4.0",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "jumptwentyfour/php-coding-standards": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3 || ^8.2 || ^9.3"
    }
}
