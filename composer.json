{
    "name": "jumptwentyfour/laravel-coding-standards",
    "description": "Coding standards for our PHP applications.",
    "license": "MIT",
    "authors": [
        {
            "role": "Technical Lead",
            "name": "Alex Williams",
            "email": "alex@jump24.co.uk",
            "homepage": "https://jump24.co.uk/"
        },
        {
            "role": "Developer",
            "name": "Daniel Newns",
            "email": "dan@jump24.co.uk",
            "homepage": "https://jump24.co.uk/"
        }
    ],
    "autoload": {
        "psr-4": {
            "JumpTwentyFour\\LaravelCodingStandards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "JumpTwentyFour\\LaravelCodingStandards\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "larastan/larastan": "^2.7.0",
        "nikic/php-parser": "^5.0",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
        "jumptwentyfour/php-coding-standards": "^v3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
